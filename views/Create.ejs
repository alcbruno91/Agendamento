<body>
    <h1>Cadastro de consulta</h1>
    <br>
    <form method="POST" action="/create">
        <input type="text" name="name" id="name" placeholder="Nome do paciente" class="form-control"><br>
        <input type="email" name="email" id="email"  placeholder="email@email.com" class="form-control"><br>
        <input type="text" name="cpf" id="cpf" placeholder="CPF" class="form-control cpf"><br>
        <input type="text" name="description" id="description" placeholder="DescriÃ§Ã£o" class="form-control"><br>
        <input type="date" name="date" id="date" placeholder="Data" class="form-control"><br>
        <input type="time" name="time" id="time" class="form-control"><br>
        <button class="btn btn-success ">Agendar</button>
    </form>
    
</body>
